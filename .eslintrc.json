{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": ["react", "prettier"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "jsx": true
    },
    "rules": {
      "prettier/prettier": "error",
      "no-console": "warn",
      "react/react-in-jsx-scope": "off"
    }
  }
  